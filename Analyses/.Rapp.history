{#
	P <- 57#
	P_supply <- 35#
	P_util <- 22#
#
	summarize <- function(en, f = in1){#
		i <- 2#
		Q <- vector()#
		while (i <= length(f)){#
			for (j in en){#
				Q <- append(Q, values = sum(j[i]))#
			}#
			i <- i + 1#
		}#
		return(Q)#
	}#
	Q_sale <- summarize(entire2) #
	Q_supply <- summarize(entire1)#
	Q_util <- Q_supply - Q_sale#
	TR <- Q_sale * P#
	TC <- Q_supply * P_supply + Q_util * P_util#
	Pr <- TR - TC#
	k <- 1#
	v <- 7#
	l <- 1#
	uniformity <- vector()#
	uniformity1 <- vector()#
	while(k <= length(entire2)){#
		h <- entire2[[k]]#
		for (j in 2 : ncol(h)){#
			for (i in 1 : nrow(h)){			#
				uniformity <- append(uniformity, h[i,j])#
			}#
			uniformity1 <- append(uniformity1, sd(uniformity[k : v]))#
				uniformity <- uniformity[1:l]#
				v <- v + 1#
				l <- l + 1#
		}#
		k <- k + 1#
	}#
#
	enter <- function(x, f = in1, table = res.tab, number){#
		i <- 1#
		k <- 10#
		while (i < (length(f) - 1) * 10){#
			x <- append(x, values = c(sum(x[i:k]), mean(x[i:k])), after = k)#
			i <- i + 12#
			k <- k + 12#
		}#
		res.tab[[number]] <- x#
		return(res.tab)#
	}#
#
	res.tab <- enter(Q_sale, f = in1, table = res.tab, 3)#
	res.tab <- enter(TR, f = in1, table = res.tab, 1)#
	res.tab <- enter(Pr, f = in1, table = res.tab, 2)#
	res.tab <- enter(Q_util, f = in1, table = res.tab, 4)#
	res.tab <- enter(uniformity1, f = in1, table = res.tab, 5)#
#
	j <- 1#
	i <- 2#
	max_util <- vector()#
	day_max_util <- vector()#
	while (i <= length(in1)){#
		while (j <= length(entire1)){#
			x <- entire1[[j]]#
			y <- entire2[[j]]#
			max_util <- append(max_util, max(x[i] - y[i]))#
			day_max_util <- append(day_max_util, which((x[i] - y[i]) == max(x[i] - y[i])))#
			j <- j + 1#
		}#
		i <- i + 1#
		j <- 1#
	}#
}#
