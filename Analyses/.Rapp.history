		e <- c('Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Suturday', 'Sunday')#
		y <- c('Понедельник', 'Вторник', 'Среда', 'Четверг', 'Пятница', 'Суббота', 'Воскресенье')#
		ee <- c('Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sut', 'Sun')#
		yy <- c('Пн', 'Вт', 'Ср', 'Чт', 'Пт', 'Сб', 'Вcк')#
		russian <- c('RU', 'ru', 'rus', 'ру', 'рус')#
		english <- c('Eng', 'eng', 'English', 'english', 'англ', 'Англ', 'анг')#
		if (k %in% english) {#
			if (x > 7){#
				t <- e[x %% 7]#
				k <- ee[x %% 7]#
				if (isDebug) {#
					return(print(k))#
				}#
				else {#
					return(print(t))#
				}  #
			else if (x < 1){#
			return(print(" "))#
			}#
			else if (is.integer(x) == FALSE){#
				if (isDebug){#
					return(print(ee[round[x]]))#
				else{#
					return(print(e[round(x)]))#
				}#
			}#
			else if ((x < 8) & (x >= 1)) {#
				t <- e[x]#
				k <- ee[x]#
				if (isDebug){#
					return(print(k))#
				}#
				else {#
					return(print(t))#
				}#
			}#
		}#
		else if (k %in% russian){#
			if (x > 7){#
				z <- y[x %% 7]#
				return(print(z))#
				}#
			else if (x < 1){#
				return(print(" "))#
			}#
			else if (is.integer(x) == FALSE){#
				return(print(y[round(x)]))#
			}#
			else if ((x < 8) & (x >= 1)) {#
				z <- y[x]#
				return(print(z))#
			}#
		}#
	}#
	day_of_week(6, 'Eng', T)#
}
