{#
	day_of_week <- function(x, y){#
		y <- c('Понедельник', 'Вторник', 'Среда', 'Четверг', 'Пятница', 'Суббота', 'Воскресенье')#
		if (x > 7){#
			z <- which((x %% 7) == y)#
			return(print(z))#
		}#
		else if (x < 1){#
			return(print(" "))#
		}#
		else if (is.integer(x) == FALSE){#
			return(print(round(x)))#
		}#
		else{#
			return(print(which( x == y)))#
		}#
	}#
	day_of_week(4)#
}
